ex1
first_num = 9
second_num = 7.8
may_str = "start"
print (first_num,second_num,may_str)
first_num = 5.2
print (first_num, float)
third_num = first_num + second_num
print (third_num,float)
first_num += 5
second_num += first_num
print(first_num, second_num)
second_num = int(second_num)
print(first_num, second_num)
may_str = "start" + "&stoo"
print (may_str)
may_str *= 5
print (may_str)
